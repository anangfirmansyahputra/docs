{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Coldsire",
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg",
    "href": "/home"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#DB7E25",
    "light": "#DB7E25",
    "dark": "#000",
    "anchors": {
      "from": "#1B676C",
      "to": "#DB7E25"
    },
    "background": {
      "dark": "#000",
      "light": "#fff"
    }
  },
  "modeToggle": {
    "default": "light",
    "isHidden": false
  },
  "backgroundImage": "/images/bg.png",
  "topbarLinks": [
    {
      "name": "Support",
      "url": "mailto:hi@mintlify.com"
    }
  ],
  "topbarCtaButton": {
    "name": "Dashboard",
    "url": "https://coldsire.com/dashboard"
  },
  "tabs": [
    {
      "name": "API Reference",
      "url": "docs/api-reference"
    }
  ],
  "anchors": [
    {
      "name": "Documentation",
      "icon": "book-open-cover",
      "url": "https://mintlify.com/docs"
    },
    {
      "name": "Community",
      "icon": "slack",
      "url": "https://mintlify.com/community"
    },
    {
      "name": "Blog",
      "icon": "newspaper",
      "url": "https://mintlify.com/blog"
    }
  ],
  "navigation": [
    {
      "group": "Get Started",
      "pages": [
        "docs/introduction",
        "docs/quickstart",
        "docs/development"
      ]
    },
    {
      "group": "Essentials",
      "pages": [
        "docs/essentials/markdown",
        "docs/essentials/code",
        "docs/essentials/images",
        "docs/essentials/settings",
        "docs/essentials/navigation"
      ]
    },
    {
      "group": "API Documentation",
      "pages": [
        "docs/api-reference/introduction"
      ]
    },
    {
      "group": "Endpoint Examples",
      "pages": [
        "docs/api-reference/endpoint/get",
        "docs/api-reference/endpoint/create",
        "docs/api-reference/endpoint/delete"
      ]
    }
  ],
  "footerSocials": {
    "twitter": "https://twitter.com/mintlify",
    "github": "https://github.com/mintlify",
    "linkedin": "https://www.linkedin.com/company/mintsearch"
  },
  "redirects": [
    {
      "source": "/",
      "destination": "/docs/introduction"
    }
  ]
}